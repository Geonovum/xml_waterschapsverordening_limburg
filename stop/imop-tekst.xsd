<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified"
  targetNamespace="https://standaarden.overheid.nl/stop/imop/tekst/" version="1.0.0"
  xmlns:tekst="https://standaarden.overheid.nl/stop/imop/tekst/"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:annotation>
    <xs:documentation>"Data-schema" van STOP. </xs:documentation>
    <xs:documentation>Dit schema definieert de "data"-elementen voor de STOP-modules</xs:documentation>
    <xs:documentation>versie 1.0.0 uitgegeven op 03-04-2020</xs:documentation>
    <xs:documentation>Veranderingen tov uitgeleverd 0.98-kern versie worden beschreven op <![CDATA[https://gitlab.com/koop/lvbb/bronhouderkoppelvlak.git/go/goto.html?to=stop&id=xsd%3Atekst]]></xs:documentation>
  </xs:annotation>
  
  <!--  ELEMENTEN -->
  <!-- ROOT REGELINGEN -->
  <xs:element name="RegelingKlassiek">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element minOccurs="0" ref="tekst:Aanhef"/>
        <xs:element minOccurs="1" ref="tekst:Lichaam"/>
        <xs:element ref="tekst:Sluiting"/>
        <xs:element minOccurs="0" name="WijzigBijlage">
          <!-- WijzigBijlage alleen voor teksten als: "Deze bijlage bevat wijzigen van die en die regeling"
          Lokale definitie? -->
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="1" ref="tekst:Kop"/>
              <xs:element minOccurs="1" ref="tekst:RegelingMutatie"/>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
        <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="RegelingCompact">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element ref="tekst:Lichaam"/>
        <!-- Structuur met ALLEEN Artikel - Valideren met Schematron!
               (WijzigArtikel is niet toegestaan). Een structuur opnemen met alle hiërarchische mogelijkheden zonder een wijzigartikel nu niet te doen.  -->
        <xs:group ref="tekst:mgBijlagenVoorRegeling"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="RegelingTijdelijkdeel">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element name="Lichaam">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="tekst:Conditie"/>
              <xs:group ref="tekst:mgLichaamZonderWijziging"/>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="RegelingVrijetekst">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element name="Lichaam">
          <xs:complexType>
            <xs:sequence>
              <xs:choice>
                <xs:group ref="tekst:mgDivisieStructuur"/>
              </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
      <xs:attributeGroup ref="tekst:agComponentNieuweRegeling"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE ROOT REGELINGEN -->
  <!-- ROOT BESLUITEN -->
  <xs:element name="BesluitKlassiek">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:RegelingOpschrift"/>
        <xs:element minOccurs="0" ref="tekst:Aanhef"/>
        <xs:choice minOccurs="1">
          <xs:element ref="tekst:Lichaam"/>
          <xs:element ref="tekst:RegelingKlassiek"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
        <xs:element minOccurs="0" ref="tekst:WijzigBijlage"/>
        <xs:group ref="tekst:mgBijlagenVoorBesluit"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="BesluitCompact">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:RegelingOpschrift"/>
        <xs:element minOccurs="0" ref="tekst:Aanhef"/>
        <xs:element ref="tekst:Lichaam"/>
        <xs:element ref="tekst:Sluiting"/>
        <xs:element maxOccurs="unbounded" minOccurs="0" name="WijzigBijlage">
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="1" ref="tekst:Kop"/>
              <xs:choice>
                <xs:element ref="tekst:RegelingMutatie"/>
                <xs:group ref="tekst:mgRegelingModellen"/>
              </xs:choice>
            </xs:sequence>
            <xs:attributeGroup ref="tekst:agAKN"/>
          </xs:complexType>
        </xs:element>
        <xs:group ref="tekst:mgBijlagenVoorBesluit"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE ROOT BESLUITEN -->
  <xs:element name="Conditie">
    <xs:annotation>
      <xs:documentation>Beschrijving: "wanneer geldt het tijdelijk deel"</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="tekst:mgLichaamZonderWijziging"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="OfficielePublicatie">
    <xs:annotation>
      <xs:documentation>Root element - voor Officiële Publicaties die door LVBB worden
        gemaakt.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="tekst:mgBladen"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agSchemaVersie"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE ROOT -->
  <!-- PUBLICATIES -->
  <xs:element name="Staatscourant">
    <xs:annotation>
      <xs:documentation>Een publicatie in de Staatscourant. Mag niet worden gebruikt als bundeling
        van meerdere publicaties die samen een Staatscourant vormen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Staatsblad">
    <xs:annotation>
      <xs:documentation>Een in het Staatsblad gepubliceerde regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Gemeenteblad">
    <xs:annotation>
      <xs:documentation>Een in het Gemeenteblad gepubliceerde regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Provincieblad">
    <xs:annotation>
      <xs:documentation>Een in het Provinciaal blad gepubliceerde regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Waterschapsblad">
    <xs:annotation>
      <xs:documentation>Een in een Waterschapsblad gepubliceerde regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="BladGR">
    <xs:annotation>
      <xs:documentation>Een in een publicatie van een gemeente gepubliceerde
        regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:group ref="tekst:mgBlad"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Bladaanduiding">
    <xs:annotation>
      <xs:documentation>De tekstuele duiding van het blad waarin de publicatie wordt geplaatst.
        Bevat 1 of 2 titelregels.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="2" ref="tekst:Titelregel"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!-- -->
  <xs:element name="Titelregel">
    <xs:annotation>
      <xs:documentation>De officiele titel en eventuele subtitel voor een publicatie. Bijvoorbeeld:
        "GEMEENTEBLAD" en in de 2e titelregel "Officiële uitgave van gemeente
        [...]".</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE PUBLICATIES -->
  <!-- ELEMENTEN FORMEEL -->
  <xs:element name="Bijlage">
    <xs:annotation>
      <xs:documentation>Binnen 'Regeling' bijlage(n) die formeel tot de regeling behoren (bindend
        zijn). Binnen 'Nawerk' een toelichtende bijlage.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Samenvatting"/>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
              <xs:element ref="tekst:Divisie"/>
              <xs:element ref="tekst:Divisietekst"/>
            </xs:choice>
          </xs:sequence>
        </xs:choice>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Toelichting">
    <xs:annotation>
      <xs:documentation>Binnen 'Regeling': toelichtingen die geconsolideerd worden. Direct binnen
        'Besluit': een toelichting die NIET geconsolideerd wordt.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:Kop"/>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Samenvatting"/>
          <xs:element maxOccurs="1" minOccurs="0" ref="tekst:AlgemeneToelichting"/>
          <xs:element maxOccurs="1" minOccurs="0" ref="tekst:ArtikelgewijzeToelichting"/>
        </xs:sequence>
        <xs:choice maxOccurs="unbounded" minOccurs="0">
          <xs:element ref="tekst:Divisie"/>
          <xs:element ref="tekst:Divisietekst"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="AlgemeneToelichting">
    <xs:annotation>
      <xs:documentation>Toelichting van algemene strekking - lees: niet
        artikelsgewijs.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:Kop"/>
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Samenvatting"/>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Divisie"/>
            <xs:element ref="tekst:Divisietekst"/>
          </xs:choice>
        </xs:sequence>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="ArtikelgewijzeToelichting">
    <xs:annotation>
      <xs:documentation>Toelichting per specifiek onderdeel, met name artikelen - lees: niet
        algemeen van aard.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice>
          <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Samenvatting"/>
            <xs:choice maxOccurs="unbounded" minOccurs="1">
              <xs:element ref="tekst:Divisie"/>
              <xs:element ref="tekst:Divisietekst"/>
            </xs:choice>
          </xs:sequence>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!-- -->
  <xs:element name="Divisietekst">
    <xs:annotation>
      <xs:documentation>Een Divisietekst bevat juridisch authentieke tekst of tekst hieraan
        gerelateerd.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:Kop"/>
        <xs:choice maxOccurs="1">
          <xs:element ref="tekst:Inhoud"/>
          <xs:element ref="tekst:Gereserveerd"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Divisie">
    <xs:annotation>
      <xs:documentation>Divisie is de recursieve hiërarchische structuur voor alle tekst met
        uitzondering van de Artikel structuur.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Samenvatting"/>
            <xs:choice maxOccurs="unbounded" minOccurs="0">
              <xs:element ref="tekst:Divisie"/>
              <xs:element ref="tekst:Divisietekst"/>
            </xs:choice>
          </xs:sequence>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Samenvatting">
    <xs:annotation>
      <xs:documentation>Een samenvattende tekst beschrijft wat de het doel of de content is van een
        structuur-element, te weten: een Bijlage, Toelichting, Motivering, Divisie. Vervangt het
        gebruik van een anoniem element Inhoud, dat een betekenisloze container
        is.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
        <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Inhoud"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAKN"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Inhoud">
    <xs:annotation>
      <xs:documentation>Inhoud is het container element waarbinnen de tekst-objecten zijn
        gegroepeerd.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded" minOccurs="1">
        <xs:group ref="tekst:mgBlocksMinimaal"/>
        <xs:group ref="tekst:mgBlocksVolledig"/>
        <xs:element ref="tekst:Kadertekst"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Kadertekst">
    <xs:annotation>
      <xs:documentation>Een "kadertekst" bevat doorgaans een nadere toelichtende tekst of een
        opsomming van ‘feiten’ om het onderwerp van de hoofdtekst nader te kunnen
        duiden.</xs:documentation>
      <xs:documentation>Dit element bepaalt NIET de wijze van weergeven voor een
        publicatie.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Kop"/>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:group ref="tekst:mgBlocksMinimaal"/>
          <xs:group ref="tekst:mgBlocksVolledig"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agAKN"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Gereserveerd">
    <xs:annotation>
      <xs:documentation>Wanneer een structuur element (Zoals Hoofdstuk, Paragraaf etc. en Bijlage en
        Divisie) nog geen content heeft, maar al wel is gereserveerd dan wordt dat met dit element
        aangegeven.</xs:documentation>
      <xs:documentation>Wanneer Gereserveerd is gebruikt dan is andere inhoud NIET
        toegestaan.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Vervallen">
    <xs:annotation>
      <xs:documentation>Wanneer een Artikel is vervallen en daarmee geen content meer heeft dan
        wordt dat aangegeven door het element Vervallen te plaatsen in plaats van de
        content.</xs:documentation>
      <xs:documentation>Wanneer Vervallen is gebruikt dan is andere inhoud NIET
        toegestaan.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="WijzigBijlage">
    <xs:annotation>
      <xs:documentation>Binnen een WijzigBijlage worden alle wijzigingen voor een regeling
        opgenomen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Kop"/>
        <xs:choice>
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:RegelingMutatie"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="RegelingMutatie">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="tekst:VoegToe"/>
        <xs:element ref="tekst:Vervang"/>
        <xs:element ref="tekst:VervangKop"/>
        <xs:element ref="tekst:Verwijder"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agComponentMutatie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="VervangKop">
    <xs:annotation>
      <xs:documentation>Een Kop die in zijn geheel wordt vervangen bij het wijzigen van structuur
        voor een regeltekst.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:sequence minOccurs="1">
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:element ref="tekst:Kop"/>
      </xs:sequence>
      <xs:attribute name="wat"/>
      <xs:attribute name="eId-wordt" type="xs:NMTOKEN" use="optional"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Verwijder">
    <xs:annotation>
      <xs:documentation>De structuur van een fragment van een regeling dat bij wijziging moet worden
        verwijderd.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:choice>
          <xs:group ref="tekst:mgStructuurRegeling"/>
          <xs:element ref="tekst:Lichaam"/>
          <xs:element ref="tekst:Bijlage"/>
          <xs:element ref="tekst:Toelichting"/>
          <xs:element ref="tekst:AlgemeneToelichting"/>
          <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="wat" use="required"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="VoegToe">
    <xs:annotation>
      <xs:documentation>De structuur van een fragment van een regeling dat bij wijziging moet worden
        toegevoegd.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:choice minOccurs="0">
          <xs:group ref="tekst:mgStructuurRegeling"/>
          <xs:element ref="tekst:Lichaam"/>
          <xs:element maxOccurs="unbounded" ref="tekst:Bijlage"/>
          <xs:element ref="tekst:Toelichting"/>
          <xs:element ref="tekst:AlgemeneToelichting"/>
          <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agWijzigLocatie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Vervang">
    <xs:annotation>
      <xs:documentation>De structuur van (een fragment van) een regeling dat bij wijziging een
        bestaande regeling wordt vervangen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:sequence>
          <xs:element minOccurs="0" ref="tekst:Nummer"/>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
        </xs:sequence>
        <xs:choice>
          <xs:group ref="tekst:mgStructuurRegeling"/>
          <xs:element ref="tekst:Lichaam"/>
          <xs:element ref="tekst:Bijlage"/>
          <xs:element ref="tekst:Toelichting"/>
          <xs:element ref="tekst:AlgemeneToelichting"/>
          <xs:element ref="tekst:ArtikelgewijzeToelichting"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="wat" use="required"/>
      <xs:attribute fixed="1" name="revisie" use="optional"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Wat">
    <xs:annotation>
      <xs:documentation>Tekstuele beschrijving van de wijziging(en) in een WijzigArtikel of een
        WijzigLid</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Kop">
    <xs:annotation>
      <xs:documentation>Voor het coderen van koppen (als lopende tekst), met drie onderdelen:
        Opschrift, Nummer, en Label.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="1">
          <xs:sequence>
            <xs:element ref="tekst:Label"/>
            <xs:element minOccurs="0" ref="tekst:Nummer"/>
            <xs:element minOccurs="0" ref="tekst:Opschrift"/>
          </xs:sequence>
          <xs:sequence>
            <xs:element ref="tekst:Nummer"/>
            <xs:element minOccurs="0" ref="tekst:Label"/>
            <xs:element minOccurs="0" ref="tekst:Opschrift"/>
          </xs:sequence>
          <xs:element minOccurs="1" ref="tekst:Opschrift"/>
        </xs:choice>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Subtitel"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Label">
    <xs:annotation>
      <xs:documentation>Het Label voor een kop-structuur, bijvoorbeeld: "Artikel", "Hoofdstuk",
        "Paragraaf" etc.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Nummer">
    <xs:annotation>
      <xs:documentation>Het Nummer voor een kop-structuur, bijvoorbeeld: "1", "3.2", "A" etc. Het
        plaatsen van een punt "." als laatste karakter wordt afgeraden.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesRegulier"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Opschrift">
    <xs:annotation>
      <xs:documentation>Het opschrift (of de titel) voor een kop-structuur.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Subtitel">
    <xs:annotation>
      <xs:documentation>Optioneel te plaatsen tweede titel regel in een
        kop-structuur.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="RegelingOpschrift">
    <xs:annotation>
      <xs:documentation>Het formele opschrift (of de titel) voor een besluit of
        regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Al"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Aanhef">
    <xs:annotation>
      <xs:documentation>De formele begin tekst voor een besluit.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:element ref="tekst:Al"/>
          <xs:element ref="tekst:Lijst"/>
        </xs:choice>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Considerans"/>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Afkondiging"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Considerans">
    <xs:complexType>
      <xs:sequence>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:element ref="tekst:Al"/>
          <xs:element ref="tekst:Lijst"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Afkondiging">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded" minOccurs="1">
        <xs:element ref="tekst:Al"/>
        <xs:element ref="tekst:Lijst"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Groep">
    <xs:annotation>
      <xs:documentation>Groepering van bijeen te houden elementen in een tekst, zonder
        tussenliggende witruimte. Via '@class' kan een vrije waarde worden toegekend, o.m. voor
        visualisatie.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded" minOccurs="1">
        <xs:group ref="tekst:mgGroepStructuur"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Lijst">
    <xs:annotation>
      <xs:documentation>Een Lijst is een recursieve structuur. Indien de lijst-items worden
        voorafgegaan door een symbool of nummering moet deze expliciet in het lijst-item (Li) worden
        opgenomen als een LiNummer.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:Lijstaanhef"/>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:element ref="tekst:Li"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="tekst:Lijstsluiting"/>
      </xs:sequence>
      <xs:attribute name="type" type="tekst:wlTypeLijst" use="required">
        <xs:annotation>
          <xs:documentation>Waarde 'expliciet' is de meest voorkomende situatie.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Lijstaanhef">
    <xs:annotation>
      <xs:documentation>Tekst waarbinnen de betekenis of het doel van de opsomming wordt beschreven.
        Maximaal 1 alinea die moet eindigen op ":".</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Li">
    <xs:annotation>
      <xs:documentation>Onderdeel voor een Lijst. Kan weer een Lijst kan bevatten, waardoor de
        lijststructuur recursief wordt.</xs:documentation>
      <xs:documentation>Gebruik van tekst:table binnen een Li is toegestaan vanwege legacy waarin
        dit voorkomt. Voor nieuwe content niet gebruiken!</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:LiNummer"/>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:element ref="tekst:Al"/>
          <xs:element ref="tekst:Figuur"/>
          <xs:element ref="tekst:Formule"/>
          <xs:element ref="tekst:Lijst"/>
          <xs:element ref="tekst:Begrippenlijst"/>
          <xs:element ref="tekst:table"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="LiNummer">
    <xs:annotation>
      <xs:documentation>Het nummer of het af te beelden symbool voor een Lijst. Gebruikte nummering
        of symbool is altijd hetzelfde op hetzelfde niveau van de Lijst.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
        <xs:element ref="tekst:sup"/>
        <xs:element ref="tekst:sub"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Lijstsluiting">
    <xs:annotation>
      <xs:documentation>Optioneel afsluitende alinea voor een Lijst.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="VerwijderdeTekst">
    <xs:annotation>
      <xs:documentation>Groepering voor tekst in een regeltekst die is verwijderd bij een
        wijziging.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="NieuweTekst">
    <xs:annotation>
      <xs:documentation>Groepering voor tekst in een regeltekst die wordt geplaatst bij een
        wijziging.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Citaat">
    <xs:annotation>
      <xs:documentation>Citaat is een letterlijke overname van tekst uit een andere publicatie.
        Aanhalingstekens maken geen onderdeel uit van de tekst van een citaat; deze worden
        gegenereerd.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgBlocksMinimaal"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agCitaat"/>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Ondertekening">
    <xs:annotation>
      <xs:documentation>De formele ondertekening voor een besluit.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Al"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Lichaam">
    <xs:annotation>
      <xs:documentation>Lichaam is het container element voor artikel(en) en eventuele groeperende
        niveau's ertussenin (Hoofdstuk, Paragraaf, etc.).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:group ref="tekst:mgLichaamStructuur"/>
        <xs:element minOccurs="0" ref="tekst:Redactioneel"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Boek">
    <xs:annotation>
      <xs:documentation>Representeert een 'Boek' in een Regeling. Volgens de AvR zijn alleen
        "Hoofdstuk" en "Deel" toegestaan als lagere niveau's.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="1" minOccurs="1">
            <xs:element maxOccurs="unbounded" ref="tekst:Deel"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Hoofdstuk"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Paragraaf"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Afdeling"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Titel"/>
          </xs:choice>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Artikel"/>
            <xs:element ref="tekst:Redactioneel"/>
            <xs:element ref="tekst:WijzigArtikel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Deel">
    <xs:annotation>
      <xs:documentation>Representeert een 'Deel' in een Regeling. Volgens de AvR is alleen
        "Hoofdstuk" als lagere niveau toegestaan.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="1" minOccurs="1">
            <xs:element maxOccurs="unbounded" ref="tekst:Boek"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Hoofdstuk"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Paragraaf"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Afdeling"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Titel"/>
          </xs:choice>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Artikel"/>
            <xs:element ref="tekst:Redactioneel"/>
            <xs:element ref="tekst:WijzigArtikel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Hoofdstuk">
    <xs:annotation>
      <xs:documentation>Representeert een 'Hoofdstuk' in een Regeling. Volgens de AvR zijn alleen
        "Afdeling", "Paragraaf"; "Titel" en "Artikel" als lagere niveau's
        toegestaan.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="1" minOccurs="0">
            <xs:element maxOccurs="unbounded" ref="tekst:Boek"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Deel"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Paragraaf"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Afdeling"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Titel"/>
          </xs:choice>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Artikel"/>
            <xs:element ref="tekst:Redactioneel"/>
            <xs:element ref="tekst:WijzigArtikel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Afdeling">
    <xs:annotation>
      <xs:documentation>Representeert een 'Afdeling' in een Regeling. Volgens de AvR is alleen
        "Paragraaf" als lager niveau toegestaan.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="1" minOccurs="0">
            <xs:element maxOccurs="unbounded" ref="tekst:Boek"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Deel"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Hoofdstuk"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Paragraaf"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Titel"/>
          </xs:choice>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Artikel"/>
            <xs:element ref="tekst:Redactioneel"/>
            <xs:element ref="tekst:WijzigArtikel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Titel">
    <xs:annotation>
      <xs:documentation>Representeert een 'Titel' in een Regeling. Volgens de AvR is alleen
        "Afdeling" als lager niveau toegestaan.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="1" minOccurs="0">
            <xs:element maxOccurs="unbounded" ref="tekst:Boek"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Deel"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Hoofdstuk"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Paragraaf"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Afdeling"/>
          </xs:choice>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Artikel"/>
            <xs:element ref="tekst:Redactioneel"/>
            <xs:element ref="tekst:WijzigArtikel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Paragraaf">
    <xs:annotation>
      <xs:documentation>Representeert een 'paragraaf' in een Regeling. Volgens de AvR is alleen
        "Artikel" als lager niveau toegestaan.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="1" minOccurs="0">
            <xs:element maxOccurs="unbounded" ref="tekst:Subparagraaf"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Subsubparagraaf"/>
            <xs:element maxOccurs="unbounded" ref="tekst:Artikel"/>
          </xs:choice>
          <xs:choice maxOccurs="unbounded" minOccurs="0">
            <xs:element ref="tekst:Redactioneel"/>
            <xs:element ref="tekst:WijzigArtikel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Subparagraaf">
    <xs:annotation>
      <xs:documentation>Representeert een 'Subparagraaf' in een Regeling. Volgens de AvR een niet
        toegestaan niveau. Aanbeveling is alleen "Subsubparagraaf" of "Artikel" te gebruiken als
        lagere niveau's.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element ref="tekst:Subsubparagraaf"/>
            <xs:element ref="tekst:Artikel"/>
            <xs:element ref="tekst:Redactioneel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Subsubparagraaf">
    <xs:annotation>
      <xs:documentation>Representeert een 'Subsubparagraaf' in een Regeling. Volgens de AvR een niet
        toegestaan niveau. Aanbeveling is alleen "Artikel" te gebruiken als lager
        niveau.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element ref="tekst:Artikel"/>
            <xs:element ref="tekst:Redactioneel"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Artikel">
    <xs:annotation>
      <xs:documentation>'Artikel' is het primaire niveau voor een regeling
        (regeltekst).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:Kop"/>
        <xs:choice minOccurs="1">
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Gereserveerd"/>
          <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Vervallen"/>
          <xs:choice minOccurs="1">
            <xs:element maxOccurs="unbounded" ref="tekst:Lid"/>
            <xs:element ref="tekst:Inhoud"/>
          </xs:choice>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Lid">
    <xs:annotation>
      <xs:documentation>'Lid' is als component voor 'Artikel' het laagste niveau voor een
        regeltekst.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:LidNummer"/>
        <xs:element ref="tekst:Inhoud"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="LidNummer">
    <xs:annotation>
      <xs:documentation>De nummering voor artikelleden.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:element ref="tekst:VerwijderdeTekst"/>
        <xs:element ref="tekst:NieuweTekst"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="WijzigArtikel">
    <xs:annotation>
      <xs:documentation>Wijzigingen voor een besluit of regeling. Tekstuele beschrijving van een of
        meer wijzigingsinstructies. Specificatie van de wijzigingen door gebruik te maken van de
        elementen 'Vervang'; 'Voegtoe'; 'Verwijder' of in een 'WijzigBijlage'.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="1" ref="tekst:Kop"/>
        <xs:element minOccurs="1" ref="tekst:Wat"/>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:OpmerkingVersie"/>
        <xs:choice minOccurs="0">
          <xs:element maxOccurs="unbounded" ref="tekst:WijzigLid"/>
          <xs:element maxOccurs="1" ref="tekst:WijzigInstructies"/>
          <xs:element maxOccurs="1" ref="tekst:RegelingMutatie"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="WijzigLid">
    <xs:annotation>
      <xs:documentation>Een of meer wijzigingsinstructie(s) binnen het besluit of regeling aangeduid
        in het bovenliggende 'WijzigArtikel'.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:LidNummer"/>
        <xs:choice>
          <xs:element minOccurs="0" ref="tekst:Wat"/>
          <xs:element ref="tekst:WijzigInstructies"/>
          <xs:element ref="tekst:RegelingMutatie"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="WijzigInstructies">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Wat"/>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Instructie"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Instructie">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="1" minOccurs="0" ref="tekst:Nummer"/>
        <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Wat"/>
        <xs:choice maxOccurs="1" minOccurs="0">
          <xs:group ref="tekst:mgInstructieStructuur"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="OpmerkingVersie">
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlineReferenties"/>
        <xs:group ref="tekst:mgInlinesRegulier"/>
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Redactioneel">
    <xs:annotation>
      <xs:documentation>Notities of opmerkingen die geen onderdeel van de (formele-)tekst of
        structuur uitmaken. Het wordt ten sterkste afgeraden hier gebruik van te
        maken.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Al"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Sluiting">
    <xs:annotation>
      <xs:documentation>Met het slotformulier, dagtekening en afsluitende (procedurele) tekst over
        de Regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice maxOccurs="unbounded" minOccurs="0">
          <xs:element ref="tekst:Groep"/>
          <xs:element ref="tekst:Al"/>
          <xs:element ref="tekst:Slotformulering"/>
          <xs:element ref="tekst:Dagtekening"/>
        </xs:choice>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Ondertekening"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Slotformulering">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded" minOccurs="1">
        <xs:element ref="tekst:Al"/>
        <xs:element ref="tekst:Groep"/>
        <xs:element ref="tekst:Lijst"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Dagtekening">
    <xs:annotation>
      <xs:documentation>Plaats en datum van ondertekening.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Al"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Motivering">
    <xs:annotation>
      <xs:documentation>Een Motivering binnen een Besluit geeft de overwegingen aan die ten
        grondslag liggen aan de wijzigingen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="tekst:Kop"/>
        <xs:choice>
          <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" ref="tekst:Inhoud"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Divisie"/>
          </xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" ref="tekst:Divisie"/>
        </xs:choice>
        <xs:element minOccurs="0" ref="tekst:Sluiting"/>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:Bijlage"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Inhoudsopgave">
    <xs:annotation>
      <xs:documentation>De Inhoudsopgave voor een Besluit of Regeling. Volgens de AvR is een
        Inhoudsopgave GEEN onderdeel van de Regeling.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:Kop"/>
        <xs:choice>
          <xs:element ref="tekst:table"/>
          <xs:element maxOccurs="unbounded" ref="tekst:Al"/>
        </xs:choice>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE ELEMENTEN FORMEEL -->
  <!-- TABLE CALS -->
  <xs:element name="table">
    <xs:annotation>
      <xs:documentation>Een tabel (conform CALS Exchange Table Model).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:title"/>
        <xs:element maxOccurs="unbounded" ref="tekst:tgroup"/>
        <xs:element minOccurs="0" ref="tekst:Bron"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agTable"/>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Bron">
    <xs:annotation>
      <xs:documentation>Als onderschrift bij een tabel of illustratie te gebruiken waarin kan worden
        aangegeven welke bron voor de tabel is gebruikt.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="tgroup">
    <xs:annotation>
      <xs:documentation>Groepering van rijen in een tabel.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" minOccurs="0" ref="tekst:colspec"/>
        <xs:element minOccurs="0" ref="tekst:thead"/>
        <xs:element ref="tekst:tbody"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agTableGroup"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="thead">
    <xs:annotation>
      <xs:documentation>Tabel rijen die de specificatie geven van de inhoud van de kolommen in de
        tabel.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="tekst:row"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agTableHead"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="tbody">
    <xs:annotation>
      <xs:documentation>De volledige inhoud voor een tabel na een header.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="tekst:row"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agTableBody"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="title">
    <xs:annotation>
      <xs:documentation>Titel voor een tabel.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesRegulier"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="colspec">
    <xs:annotation>
      <xs:documentation>Specificaties voor het weergeven van een kolom binnen een
        tabel.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="tekst:agColspec"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="row">
    <xs:annotation>
      <xs:documentation>Een rij in een tabel.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="tekst:entry"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agTableRow"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="entry">
    <xs:annotation>
      <xs:documentation>Een cel binnen een tabel.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgBlocksEntry"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agTableEntry"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE TABLE CALS -->
  <!-- TEKST CONTAINERS -->
  <xs:element name="Al">
    <xs:annotation>
      <xs:documentation>Alinea bevat de lopende tekst.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Begrippenlijst">
    <xs:annotation>
      <xs:documentation>Een lijst met definities voor begrippen.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="tekst:Begrip"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Begrip">
    <xs:annotation>
      <xs:documentation>Een Begrip is de combinatie van een term en de bijbehorende
        definitie.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:LiNummer"/>
        <xs:element ref="tekst:Term"/>
        <xs:element ref="tekst:Definitie"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Term">
    <xs:annotation>
      <xs:documentation>Het begrip of de term die nader wordt gedefinieerd in een
        begrippenlijst.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesRegulier"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Definitie">
    <xs:annotation>
      <xs:documentation>De definitie van een term in een begrippenlijst.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="tekst:Al"/>
        <xs:element ref="tekst:Lijst"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Figuur">
    <xs:annotation>
      <xs:documentation>Figuur is een container voor illustraties, inclusief bijbehorende gegevens
        voor weergave.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="Titel">
          <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded" minOccurs="0">
              <xs:group ref="tekst:mgInlinesCompleet"/>
            </xs:choice>
            <xs:attributeGroup ref="tekst:agWijzigactie"/>
            <xs:attributeGroup ref="tekst:agAlgemeen"/>
          </xs:complexType>
        </xs:element>
        <xs:element ref="tekst:Illustratie"/>
        <xs:element minOccurs="0" ref="tekst:Bijschrift"/>
        <xs:element minOccurs="0" ref="tekst:Bron"/>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Formule">
    <xs:annotation>
      <xs:documentation>Formule is een container voor de diverse vormen van formules, als tekst, of
        afbeelding. De tekstuele variant heeft een opbouw vergelijkbaar met een
        tabel.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice maxOccurs="1" minOccurs="1">
          <xs:element ref="tekst:Illustratie"/>
        </xs:choice>
        <xs:sequence>
          <xs:element minOccurs="0" ref="tekst:Bijschrift"/>
          <xs:element minOccurs="0" ref="tekst:Bron"/>
        </xs:sequence>
      </xs:sequence>
      <xs:attributeGroup ref="tekst:agAKN"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="InlineTekstAfbeelding">
    <xs:annotation>
      <xs:documentation>Een afbeelding dat in lopende tekst geplaatst moet worden (als een object).
        Bevat reeks van karakters of symbolen die niet op andere wijze kunnen worden
        afgebeeld.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="tekst:agInlineTekstAfbeelding"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Illustratie">
    <xs:annotation>
      <xs:documentation>Verwijzing naar een afbeelding (als een object).</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:attributeGroup ref="tekst:agIllustratie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Bijschrift">
    <xs:annotation>
      <xs:documentation>Een verklarend bijschrift bij een illustratie of figuur.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesCompleet"/>
      </xs:choice>
      <xs:attribute name="locatie" type="tekst:wlPositieBijschrift" use="required">
        <xs:annotation>
          <xs:documentation>Plaatsing van het bijschrift, als default is "onder" te
            gebruiken.</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Tussenkop">
    <xs:annotation>
      <xs:documentation>Tussenkop is een niet-hiërarchische kop die geplaatst kan worden in lopende
        tekst.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesRegulier"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
      <xs:attributeGroup ref="tekst:agAlgemeen"/>
    </xs:complexType>
  </xs:element>
  <!-- EINDE TEKST CONTAINERS -->
  <!-- INLINES -->
  <xs:element name="i">
    <xs:annotation>
      <xs:documentation>Een tekstfragment binnen een alinea dat cursief moet worden
        weergegeven.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:element ref="tekst:b"/>
        <xs:element ref="tekst:u"/>
        <xs:element ref="tekst:strong"/>
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
        <xs:group ref="tekst:mgInlineReferenties"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="b">
    <xs:annotation>
      <xs:documentation>Een tekstfragment binnen een alinea dat vet moet worden
        weergegeven.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:element ref="tekst:i"/>
        <xs:element ref="tekst:u"/>
        <xs:element ref="tekst:strong"/>
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
        <xs:group ref="tekst:mgInlineReferenties"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="u">
    <xs:annotation>
      <xs:documentation>Een tekstfragment binnen een alinea dat onderstreept moet worden
        weergegeven.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:element ref="tekst:b"/>
        <xs:element ref="tekst:i"/>
        <xs:element ref="tekst:strong"/>
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
        <xs:group ref="tekst:mgInlineReferenties"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="strong">
    <xs:annotation>
      <xs:documentation>Een tekstfragment binnen een alinea dat met extra nadruk moet worden
        weergegeven.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:element ref="tekst:b"/>
        <xs:element ref="tekst:u"/>
        <xs:element ref="tekst:i"/>
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
        <xs:group ref="tekst:mgInlineNoten"/>
        <xs:group ref="tekst:mgInlinesWijzigingen"/>
        <xs:group ref="tekst:mgInlineReferenties"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="sup" type="xs:string">
    <xs:annotation>
      <xs:documentation>Een karakter dat als superieur moet worden weergegeven (boven de x-lijn van
        de tekst).</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!--  -->
  <xs:element name="sub" type="xs:string">
    <xs:annotation>
