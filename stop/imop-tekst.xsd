      <xs:documentation>Een afkorting in tekst, waarbij de betekenis van de afkorting in het
        attribuut 'title' kan worden opgegeven. </xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attribute name="title" type="xs:string"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="ExtRef">
    <xs:annotation>
      <xs:documentation>ExtRef is een externe verwijzing. Het verplichte attribuut 'doel' bevat de
        URL of locatie van een document of een positie binnen een document.</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:choice maxOccurs="unbounded" minOccurs="0">
        <xs:group ref="tekst:mgInlinesZonderKinderen"/>
      </xs:choice>
      <xs:attributeGroup ref="tekst:agExtrefRefSoort"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="Noot">
    <xs:annotation>
      <xs:documentation>Een Noot waarnaar vanuit tekst wordt verwezen. Typering voor plaatsing van
        de Noot kan worden opgegeven met behulp van het attribuut 'type'.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" ref="tekst:NootNummer"/>
        <xs:choice maxOccurs="unbounded" minOccurs="1">
          <xs:element ref="tekst:Al"/>
          <xs:element ref="tekst:Lijst"/>
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="type" type="tekst:wlTypeNoot" use="required"/>
      <xs:attribute name="id" type="xs:ID" use="required"/>
      <xs:attributeGroup ref="tekst:agWijzigactie"/>
    </xs:complexType>
  </xs:element>
  <!--  -->
  <xs:element name="NootNummer" type="tekst:dtNootNummerLengte">
